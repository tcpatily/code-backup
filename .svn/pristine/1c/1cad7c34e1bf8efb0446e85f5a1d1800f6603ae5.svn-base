    <title> 
    <%
    String userSet = seoContent.getUserSet();
    String userSetNew = seoContent.getUserSetNew();
    String familyPackages = seoContent.getFamilyPkg();
	
	String cityName = null;
	if(familyPackages != null) {
		String destination = seoContent.getDestinationFromUrl();
    	cityName = destination.split("family")[0];
	}
	String count = seoContent.getTotalCount();
	
    if ("userSetNew".equals(userSetNew)) {
    %>
        <%= count + " " %> <%= seoContent.getDestinationFromUrl().replaceFirst(" from ", " Packages from ") %> Book Your Trip to <%= seoContent.getDestinationFromUrl() %>
    <%
    } else if ("FamilyPKG".equalsIgnoreCase(familyPackages)) { 
    %>
        <%= cityName %> Family Tour Packages - Plan a Relaxing Family Trip to <%= cityName %>
    <%
    } else {
    %>
        <%= seoContent.getMetaTitle() %>
    <%
    }
    %>
	
</title>	
	
