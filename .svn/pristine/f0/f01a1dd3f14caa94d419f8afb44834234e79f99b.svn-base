<!------Tour Managers------->
        <div class="tour_managers rn__section__gapTop">
            <div class="container">
                <div class="row gx-0 justify-content-center">
                    <div class="col-xxxl-9 col-xxl-12">
                        <div class="main_heading">
                            <h4>Our tour Managers</h4>
                        </div>

                        <div class="inner__gapTop">
                            <div class="managers_slide owl-carousel">
							<div class="managerloop" v-for="(manager, index) in tourManagers?.managers.slice(0,9)" :key="index">
                                <div class="card_box white_bg">
                                    <div class="left_blc">
                                        <img v-bind:src="manager.imagePath" alt="">
                                    </div>
                                    <div class="info_blc">
                                        <h5>{{manager.tourManagerName}}</h5>
                                        <div class="info_inr">
                                            <span class="title">Experience</span>
                                            <span class="sub_title">{{manager.experienceYears}}</span>
                                        </div>
                                        <div class="info_inr">
                                            <span class="title">Languages</span>
                                            <span class="sub_title">{{manager.languageSpoken}}</span>
                                        </div>
                                        <a href="" class="btn">View Details</a>
                                    </div>
                                </div>
								</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>